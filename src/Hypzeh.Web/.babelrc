{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        },
        "corejs": 3,
        "useBuiltIns": "usage",
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "babel-plugin-styled-components",
      {
        "minify": true,
        "pure": true,
      }
    ],
    "react-hot-loader/babel",
  ]
}
